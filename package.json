{
  "name": "cacombos-ui",
  "version": "1.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run cacombos:serve-ssr",
    "serve:ssr": "node dist/cacombos/server/main.js",
    "build:ssr": "ng build --configuration production && ng run cacombos:server:production",
    "build:stage:ssr": "ng build --configuration=staging && ng run cacombos:server:production",
    "prerender": "ng run cacombos:prerender",
    "typedoc": "typedoc --mode modules -module commonjs --exclude **/*.spec.ts --out docs/typedoc src/app"
  },
  "private": true,
